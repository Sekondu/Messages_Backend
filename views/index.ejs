<html>
    <body>
        <h1>All Messages</h1>
        <a href="./new">Add a new message</a>
        <% messages.forEach((message) => {
            %>
            <h2><%=message.user%></h2>
            <p><%=message.text%></p>
            <p>time <%=message.added%></p>
            <a href="/view_message?mydata=<%= encodeURIComponent(JSON.stringify(message))%>">View Message</a>
            <%
        }) %>
    </body>
</html>